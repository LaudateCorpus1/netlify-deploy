{
  "version": 2,
  "name": "netlify-deploy",
  "alias": "netlify-deploy",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NETLIFY_TOKEN": "@netlify_token",
    "NETLIFY_OAUTH_CLIENT_ID": "@netlify_client_id",
    "NETLIFY_OAUTH_CLIENT_SECRET": "@netlify_client_secret"
  }
}
